	<Frame name="LSUILineTemplate" virtual="true">
		<Layers>
			<Layer level="ARTWORK" textureSubLevel="1">
				<Line parentKey="Fill" file="Interface/Artifacts/_Artifacts-DependencyBar-Fill" thickness="16" horizTile="true"/>
			</Layer>
			<Layer level="ARTWORK" textureSubLevel="2">
				<Line parentKey="FillScroll1" file="Interface/Artifacts/_Artifacts-DependencyBar-FillScroll1" thickness="16" horizTile="true" alphaMode="ADD"/>
				<Line parentKey="FillScroll2" file="Interface/Artifacts/_Artifacts-DependencyBar-FillScroll2" thickness="16" horizTile="true" alphaMode="ADD"/>
			</Layer>
		</Layers>
		<Animations>
			<AnimationGroup parentKey="ScrollAnim" setToFinalAlpha="true" looping="REPEAT">
				<TextureCoordTranslation childKey="FillScroll1" duration="12" order="1" offsetU="1" offsetV="0"/>
				<TextureCoordTranslation childKey="FillScroll2" duration="12" order="1" offsetU="-1" offsetV="0"/>
			</AnimationGroup>
		</Animations>
		<Scripts>
			<OnLoad>
				self.SetVertexColor = function(self, r, g, b, a)
					self.Fill:SetVertexColor(r, g, b, a or 1)
					self.FillScroll1:SetVertexColor(r, g, b, a or 1)
					self.FillScroll2:SetVertexColor(r, g, b, a or 1)
				end

				self.GetVertexColor = function(self)
					return self.Fill:GetVertexColor()
				end

				self.SetStartPoint = function(self, p, parent)
					self.Fill:SetStartPoint(p, parent)
					self.FillScroll1:SetStartPoint(p, parent)
					self.FillScroll2:SetStartPoint(p, parent)
				end

				self.SetEndPoint = function(self, p, parent)
					self.Fill:SetEndPoint(p, parent)
					self.FillScroll1:SetEndPoint(p, parent)
					self.FillScroll2:SetEndPoint(p, parent)
				end

				self.SetThickness = function(self, thickness)
					self.Fill:SetThickness(thickness)
					self.FillScroll1:SetThickness(thickness)
					self.FillScroll2:SetThickness(thickness)
				end

				self:RegisterEvent("DISPLAY_SIZE_CHANGED")
				self:RegisterEvent("UI_SCALE_CHANGED")
				self:SetThickness(16 * _G.UIParent:GetScale())
			</OnLoad>
			<OnEvent>
				self:SetThickness(16 * _G.UIParent:GetScale())
			</OnEvent>
		</Scripts>
	</Frame>
</Ui>
